<h1>投稿フォーム</h1>
<%= form_with model: @post,url: public_posts_path do |f| %>
  <h4>image</h4>
  <%= f.file_field :main_images, multiple: true %>
  <h4>Title</h4>
  <%= f.text_field :title %>
  <h4>Opinion</h4>
  <%= f.text_area :body %>
  <h4>address</h4>
  <%= f.text_field :address %>
<%= f.submit '投稿する' %>
<% end %>
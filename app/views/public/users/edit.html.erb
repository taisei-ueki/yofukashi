<%= form_with model: @user, url: public_user_path, method: :patch do |f| %>

<p>プロフィール編集</p>
<table>
  <tr>
    <th></th>
    <td><%= f.file_field :profile_image, accept: 'image/*' %></td>
  </tr>
  <tr>
    <th></th>
    <td><%= f.text_field :name %></td>
  </tr>
  <tr>
    <th></th>
    <td><%= f.text_field :introduction %></td>
  </tr>
  <tr>
    <th></th>
    <td><%= f.text_field :email ,autofocus: true, autocomplete: "email" %></td>
  </tr>
</table>
<%= f.submit "編集内容を保存する" %>
<% end %>
